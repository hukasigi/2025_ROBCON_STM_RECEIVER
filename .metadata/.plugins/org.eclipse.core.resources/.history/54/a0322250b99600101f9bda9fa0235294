    /*Configure GPIO pins : DIR5_Pin DIR6_Pin */
    GPIO_InitStruct.Pin   = DIR5_Pin | DIR6_Pin;
    GPIO_InitStruct.Mode  = GPIO_MODE_OUTPUT_PP;
